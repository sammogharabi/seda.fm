# Local Development Environment Variables
# Copy this to .env.local and update with your values

# Environment Configuration
ENV_TAG=development
NODE_ENV=development

# Supabase Configuration (from local Supabase)
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your-local-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-local-service-role-key

# Admin Secret (use a secure random string)
ADMIN_SECRET=local_dev_secret_change_this

# PostHog Analytics (optional)
# IMPORTANT: Use a NEW key, not the exposed one
POSTHOG_API_KEY=phc_YOUR_NEW_DEV_KEY_HERE

# Database URL (for local development)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres